[[ #PROPS userInfo, postInfo ]]
[[ #COMPONENT .\components\componentA AS compA ]]
[[ #COMPONENT .\components\componentB AS compB ]]
<compA username={{ userInfo.username }} id={{ userInfo.id }} quote={{ userInfo.quote }} />
<compB postName={{ postInfo.postName }} postAuthor={{ userInfo.id }} postBody={{ postInfo.postBody }} />